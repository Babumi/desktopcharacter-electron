{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "electron",
            "cwd": "${workspaceRoot}",
            "env": {
                "NODE_ENV" :"development"
            },
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "program": "${workspaceRoot}/app/src/entry/main_process/electron.js"      
        },        
        {
            "name": "web-packet-service",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/webpack-dev-server/bin/webpack-dev-server.js",
            "args": [
                "--inline", "--hot", "--colors", "--port", "9080", "--content-base", "app/dist"
            ]
        }
    ]
}
